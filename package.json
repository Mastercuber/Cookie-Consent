    "@vitest/ui": "^0.26.0",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.13",
    "browserslist": "^4.21.4",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.5.1",
    "happy-dom": "^8.1.0",
    "pathe": "^1.0.0",
    "postcss": "^8.4.14",
    "postcss-nesting": "^10.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.9.4",
    "vite": "^4.0.2",
    "vite-plugin-dts": "^1.5.0",
    "vite-plugin-vue-type-imports": "^0.2.4",
    "vitepress": "^1.0.0-alpha.15",
    "vitest": "^0.26.0",
    "vue-tsc": "^1.0.14"
